{
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.associations": {
        "*.json": "json",
        ".*rc": "json"
    },
    "files.insertFinalNewline": true,
    "editor.formatOnSave": true,
    "tailwindCSS.validate": true,
    "tailwindCSS.rootFontSize": 10,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "editor.quickSuggestions": {
        "strings": true
    }
}

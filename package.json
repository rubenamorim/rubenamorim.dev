{
    "name": "nextjs-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:analyze": "ANALYZE=true yarn build",
        "start": "next start",
        "lint": "yarn eslint && yarn stylelint",
        "eslint": "next lint",
        "stylelint": "stylelint '**/*.css'",
        "test": "NODE_ENV=test TZ=utc jest",
        "test:quick": "yarn test --no-coverage",
        "test:watch": "yarn test --watch",
        "test:watch:quick": "yarn test:watch --no-coverage",
        "test:report": "open coverage/lcov-report/index.html",
        "typecheck": "tsc",
        "verify": "run-p --print-label lint typecheck test build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@next/bundle-analyzer": "^12.1.6",
        "classnames": "^2.3.1",
        "next": "^12.1.6",
        "next-compose-plugins": "^2.2.1",
        "next-seo": "^5.4.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@netlify/plugin-nextjs": "^4.11.1",
        "@testing-library/dom": "^8.13.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^14.2.0",
        "@types/jest": "^27.5.1",
        "@types/node": "16",
        "@types/react": "^18.0.10",
        "@types/testing-library__jest-dom": "^5.14.3",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.30.0",
        "autoprefixer": "^10.4.4",
        "eslint": "^8.16.0",
        "eslint-config-next": "^12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-jest": "^26.4.6",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-testing-library": "^5.5.1",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "lint-staged": "^13.0.0",
        "match-media-mock": "^0.1.1",
        "netlify-plugin-cache-nextjs": "^1.6.1",
        "npm-run-all": "^4.1.5",
        "open": "^8.4.0",
        "postcss": "^8.4.14",
        "postcss-preset-env": "^7.7.0",
        "prettier": "^2.6.2",
        "stylelint": "^14.8.5",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^8.0.0",
        "stylelint-order": "^5.0.0",
        "tailwindcss": "^3.0.24",
        "typescript": "^4.7.2"
    },
    "engines": {
        "node": "16",
        "yarn": ">=1.22"
    }
}

{
    "name": "nextjs-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:analyze": "ANALYZE=true yarn build",
        "start": "next start",
        "lint": "yarn eslint && yarn stylelint",
        "eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.json",
        "stylelint": "stylelint '**/*.css'",
        "test": "NODE_ENV=test TZ=utc jest",
        "test:quick": "yarn test --no-coverage",
        "test:watch": "yarn test --watch",
        "test:watch:quick": "yarn test:watch --no-coverage",
        "test:report": "open coverage/lcov-report/index.html",
        "typecheck": "tsc",
        "verify": "run-p --print-label lint typecheck test build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@next/bundle-analyzer": "^11.1.2",
        "classnames": "^2.3.1",
        "next": "^11.1.2",
        "next-compose-plugins": "^2.2.1",
        "next-seo": "^4.28.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.2.0",
        "@commitlint/config-conventional": "^13.2.0",
        "@netlify/plugin-nextjs": "^3.9.0",
        "@testing-library/dom": "^8.10.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.1",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
        "@types/react": "^17.0.26",
        "@types/testing-library__jest-dom": "^5.14.1",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "autoprefixer": "^10.3.6",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.5.0",
        "eslint-plugin-jest-dom": "^3.9.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^4.12.4",
        "husky": "^7.0.2",
        "jest": "^27.3.1",
        "lint-staged": "^11.2.6",
        "match-media-mock": "^0.1.1",
        "netlify-plugin-cache-nextjs": "^1.6.1",
        "npm-run-all": "^4.1.5",
        "open": "^8.2.1",
        "postcss": "^8.3.8",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.4.1",
        "prettier-eslint-cli": "^5.0.1",
        "stylelint": "^13.13.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-order": "^4.1.0",
        "tailwindcss": "^2.2.19",
        "typescript": "^4.4.4"
    },
    "engines": {
        "node": ">=14.15",
        "yarn": ">=1.22"
    }
}

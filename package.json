{
    "name": "nextjs-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:analyze": "ANALYZE=true yarn build",
        "start": "next start",
        "lint": "yarn eslint && yarn stylelint",
        "eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.json",
        "stylelint": "stylelint '**/*.css'",
        "test": "NODE_ENV=test TZ=utc jest",
        "test:quick": "yarn test --no-coverage",
        "test:watch": "yarn test --watch",
        "test:watch:quick": "yarn test:watch --no-coverage",
        "test:report": "open coverage/lcov-report/index.html",
        "typecheck": "tsc",
        "verify": "run-p --print-label lint typecheck test build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@next/bundle-analyzer": "^12.1.0",
        "classnames": "^2.3.1",
        "next": "^12.1.0",
        "next-compose-plugins": "^2.2.1",
        "next-seo": "^5.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.0.0",
        "@netlify/plugin-nextjs": "^4.2.2",
        "@testing-library/dom": "^8.11.3",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.3",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.1",
        "@types/node": "16",
        "@types/react": "^17.0.38",
        "@types/testing-library__jest-dom": "^5.14.2",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.13.0",
        "autoprefixer": "^10.4.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^12.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.0.0",
        "eslint-plugin-jest-dom": "^4.0.1",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-testing-library": "^5.0.5",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.3",
        "match-media-mock": "^0.1.1",
        "netlify-plugin-cache-nextjs": "^1.6.1",
        "npm-run-all": "^4.1.5",
        "open": "^8.4.0",
        "postcss": "^8.4.7",
        "postcss-preset-env": "^7.3.0",
        "prettier": "^2.5.1",
        "prettier-eslint-cli": "^5.0.1",
        "stylelint": "^14.3.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^6.0.0",
        "stylelint-order": "^5.0.0",
        "tailwindcss": "^3.0.18",
        "typescript": "^4.6.2"
    },
    "engines": {
        "node": "16",
        "yarn": ">=1.22"
    }
}
